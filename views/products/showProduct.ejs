<div>
    all products
</div>
<div>
    <% for(product of products){%>
        <ul>
            <li>Name: <%=product.name%></li>
            <li>Package Size: <%=product.size%></li>
            <li>Category: <%=product.category%></li>
            <li>Mrp: <%=product.mrp%></li>
            <li>Img: <%=product.img%></li>
            <img width="50" height="50" src="<%=product.img%>" alt="">
            <li>Status: <%=product.status%></li>
            <li><a href="/product/delete?id=<%=product._id%>">X</a></li>
            <li><a href="/product/update-status?id=<%=product._id%>&status=<%=product.status%>">update status</a></li>
        </ul>
    <%}%>
</div>